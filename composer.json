{
  "name": "cowegis/cowegis-geojson",
  "description": "Cowegis GeoJSON - PHP GeoJSON specification (RFC 7946) implementation",
  "license": "LGPL-3.0-or-later",
  "type": "library",
  "keywords": [
    "geosjon",
    "gis",
    "webgis"
  ],
  "authors": [
    {
      "name": "David Molineus",
      "email": "david.molineus@netzmacht.de"
    }
  ],
  "require": {
    "php": "^8.1",
    "ext-json": "*"
  },
  "require-dev": {
    "doctrine/coding-standard": "^12.0.0",
    "phpcq/runner-bootstrap": "^1.0@dev",
    "phpspec/phpspec": "^6.1 || ^7.0"
  },
  "autoload": {
    "psr-4": {
      "Cowegis\\GeoJson\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "spec\\Cowegis\\GeoJson\\": "spec/"
    }
  },
  "config": {
    "allow-plugins": {
      "dealerdirect/phpcodesniffer-composer-installer": true
    }
  },
  "extra": {
    "branch-alias": {
      "dev-master": "1.1.x-dev"
    }
  }
}
